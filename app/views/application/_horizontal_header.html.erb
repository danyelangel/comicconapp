  <div class="container">
    <div class="row">
      <div class="col-xs-12">

        <nav id="horizontal-nav" class="navbar navbar-default navbar-header" role="navigation">
          <div class="text-center hidden-lg hidden-md">
            <%= link_to image_tag("logo-header.png", alt: "Comic Con", class: "logo"), "/" %>
