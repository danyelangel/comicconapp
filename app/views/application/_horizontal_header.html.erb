          </div>

          <div class="container text-center hidden-xs hidden-sm">
            <div class="col-md-2 header-active">
              <%= link_to 'Home', '/', data: { no_turbolink: true } %>
            </div>
            <div class="col-md-2">
              <!-- <%= link_to 'Expositores', '#', data: { no_turbolink: true } %> -->
