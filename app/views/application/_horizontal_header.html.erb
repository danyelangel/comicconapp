            </div>
            <div class="col-md-2">
              <%= link_to 'Invitados', '/invitados', data: { no_turbolink: true } %>
            </div>
            <div class="col-md-2">
              <%= link_to 'ContÃ¡ctenos', '/#contactenos', data: { no_turbolink: true } %>
            </div>
            <div class="col-md-2 social-media">
              <ul class="social-media">
                <li>
                  <%= link_to 'https://www.facebook.com/ComicConColombia', alt: "Facebook Comic Con Colombia", target: "_blank" do %>
                    <i class="ion-social-facebook"></i>
                  <% end %>
                </li>
                <li>
                  <%= link_to 'https://twitter.com/comicconcol', alt: "Twitter Comic Con Colombia", target: "_blank" do %>
                    <i class="ion-social-twitter"></i>
                  <% end %>
                </li>
              </ul>
              <%= link_to image_tag("head-boletas.png", alt: "Adquiere tu boleta", class: "head-boletas"), 'http://vive.tuboleta.com/shows/show.aspx?sh=UCOMIC', target: "_blank" %>
